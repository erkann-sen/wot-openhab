            }, {
                "op": ["observeproperty"],
                "subprotocol": "sse",
                "contentType": "application/json",
                "href": "http://127.0.0.1:8080/rest/events?topics=smarthome/items/HueAmbientLightSensor1_LowBattery/statechanged"
            }]
